<!DOCTYPE html>
<html lang="en">
  <%- include('./partials/head.ejs') %>
<body>
    <%- include('./partials/header.ejs') %>
    <main>
        <h2>Contacts</h2>
        <ul>
            <% if (contacts.length) { %>
              <% contacts.forEach(({ link, name }) => { %>
                <li>
                  <a href="<%= link %>"><%= name %></a>
                </li>
              <% }) %>
            <% } %>
          </ul>
    </main>

</body>
</html>